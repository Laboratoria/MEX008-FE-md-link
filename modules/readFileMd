

const fs = require ("fs");
const typeOfFile = require('./restrictFile');

// console.log(typeOfFile())
const readFileMd =()=>{
  if(typeOfFile()){
    fs.readFile("./README.md", "utf8", (error , data) => {
      if (error) {
        console.error(error);
      }else {
        console.log(data);
      }
    });

  }else {
    console.log("archivo invalido");
  }
  };

readFileMd();



module.exports='readFileMd';



